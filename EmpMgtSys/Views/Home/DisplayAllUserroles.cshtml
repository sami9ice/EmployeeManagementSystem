@model CYBInfrastructure.Web.Model.AllroleandUser
@using GridMvc.Html


@{
    ViewBag.Title = "Display All User roles";
}

<h2>DisplayAllUserroles</h2>
<link href="~/Content/Gridmvc.css" rel="stylesheet" />

@*<link href="~/bootstrap/css/bootstrap.min.css" rel="stylesheet" />*@
<link href="~/Content/bootstrap.min.css" rel="stylesheet" />

@*<script src="~/Scripts/jquery-1.9.1.min.js"></script>*@
<script src="~/Scripts/jquery-3.3.1.min.js"></script>
<script src="~/Scripts/gridmvc.js"></script>

@Html.Grid(Model.AllDetailsUserlist).Columns(columns =>

{

    columns.Add(c => c.UserName).Titled("Staff ID").Filterable(true);


    columns.Add(c => c.StaffName).Titled("Staff Name").Filterable(true);


    columns.Add(c => c.RoleName).Titled("Role Name").Filterable(true);




}).WithPaging(10).Sortable(true)


