<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <meta charset="utf-8" />
    <title>Simulor - Responsive Admin Dashboard Template</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta content="A fully featured admin theme which can be used to build CRM, CMS, etc." name="description" />
    <meta content="Coderthemes" name="author" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <!-- App favicon -->
    @*<link rel="shortcut icon" href=@Url.Content("/Content/assets/images/favicon.ico")>*@
    @*<link rel="shortcut icon" href=@Url.Content( "~/Content/assets/images/cyberspace_logo_H7R_icon.ico")>*@
    <link rel="shortcut icon" href=@Url.Content("~/Content/assets/images/cyberspace-icon.ico"  )>

    <!-- third party css -->
    <link href="~/Content/css/vendor/toastr.min.css" rel="stylesheet" type="text/css" />
    @*<link href="~/Content/toastr.min.css" rel="stylesheet" />*@
    <link href="~/Content/css/vendor/responsive.bootstrap4.css" rel="stylesheet" />

    <link href="~/Content/css/vendor/buttons.bootstrap4.css" rel="stylesheet" />

    <link href="~/Content/css/vendor/select.bootstrap4.css" rel="stylesheet" />
    <!-- third party css end -->
    <!-- App css -->
    <link href="~/Content/css/icons.min.css" rel="stylesheet" />
    <link href="~/Content/css/app.min.css" rel="stylesheet" />
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/css/vendor/dataTables.bootstrap4.css" rel="stylesheet" />
</head>
<body>
    <div>

        <!-- Begin page -->
        <div id="wrapper">
            <!-- ========== Left Sidebar Start ========== -->
            <div class="left-side-menu">
                <div class="slimscroll-menu">
                    <!-- LOGO -->
                    <a href="/Location/Index" class="logo text-center mb-4">
                        <span class="logo-lg">
                            <img src="~/Content/assets/images/CyberSpace Logo.png" alt="" height="80">
                        </span>
                        <span class="logo-sm">
                            @*<img src="assets/images/logo-sm.png" alt="" height="24">*@
                        </span>
                    </a>
                    <!--- Sidemenu -->
                    <div id="sidebar-menu">
                        <ul class="metismenu" id="side-menu">


                            <li class="menu-title" style="color:black">CYBInfrastructure</li>

                            <li>
                                <a href="javascript: void(0);">
                                    <i class="fe-grid"></i>
                                    <span> Setup </span>
                                    <span class="menu-arrow"></span>
                                </a>
                                <ul class="nav-second-level" aria-expanded="false">
                                    <li>
                                        <a href="~/Location/Index/">Manage Location</a>
                                    </li>
                                    <li>
                                        <a href="~/Department/Index">Manage Department</a>
                                    </li>
                                    <li>
                                        <a href="~/Unit/Index">Manage Unit</a>
                                    </li>
                                    <li>
                                        <a href="~/Host/Index">Host Set-Up</a>
                                    </li>

                                </ul>
                            </li>

                            <li>
                                <a href="javascript: void(0);">
                                    <i class="fe-sidebar"></i>
                                    <span> Inventory </span>
                                    <span class="menu-arrow"></span>
                                </a>
                                <ul class="nav-second-level" aria-expanded="false">
                                    <li>
                                        <a href="~/VirtualMachine/Index">Virtual Machine</a>
                                    </li>
                                    <li>
                                        <a href="~/Service/Index">Services Offered</a>
                                    </li>

                                    <li>
                                        <a href="~/Credential/Index">Credential Set-Up</a>
                                    </li>

                                    @*<li>
                                            <a href="layouts-sm-sidebar.html">Small Sidebar</a>
                                        </li>
                                        <li>
                                            <a href="layouts-dark-sidebar.html">Dark Sidebar</a>
                                        </li>
                                        <li>
                                            <a href="layouts-light-topbar.html">Light Topbar</a>
                                        </li>*@
                                </ul>
                            </li>
                        </ul>
                    </div>

                    <!-- End Sidebar -->
                    <div class="clearfix"></div>
                </div>
                <!-- Sidebar -left -->
            </div>
            <!-- LSidebareft  End -->
            <!-- ============================================================== -->
            <!-- Start Page Content here -->
            <!-- ============================================================== -->
            <div class="content-page">
                <div class="content">
                    <!-- Topbar Start -->
                    <div class="navbar-custom">
                        <div class="navbar-header">

                            <ul class="list-unstyled  topbar-left-menu float-right mb-0">
                                <li>    <a class="btn btn-default" style=" margin-top:15px" href="/Home/Index"><i class="fa fa-plus"></i> ABOUT</a></li>
                            </ul>
                            <ul class="list-unstyled  topbar-left-menu float-right mb-0">
                                <li>    <a class="btn btn-default" style=" margin-top:15px" href="~/Location/Index/"><i class="fa fa-plus"></i> HOME</a></li>
                            </ul>
                            <ul class="list-unstyled  topbar-left-menu float-right mb-0">
                                @*<li>    <a href="#"> Welcome Mr/Mrs: @User.Identity.Name</a></li>*@
                            </ul>
                        </div>
                        <button class="button-menu-mobile open-left disable-btn">

                            <i class="fe-menu"></i>
                        </button>
                        <div class="app-search">
                            <form>


                                <div class="input-group">

                                    <input type="text" class="form-control" placeholder="Search...">
                                    <div class="input-group-append">
                                        <button class="btn btn-primary" type="submit">
                                            <i class="fe-search"></i>
                                        </button>

                                    </div>

                                </div>

                            </form>

                        </div>

                    </div>
                    <!-- end Topbar -->
                    <!-- Start Content-->
                    <div class="container-fluid">

                        <!-- start page title -->
                        <div class="row">
                            <div class="col-12">
                                <div class="page-title-box">
                                    <div class="page-title-right">
                                        <ol class="breadcrumb m-0">
                                            @*<li class="breadcrumb-item"><a href="/Account/Register">Register</a></li>*@
                                            <li class="breadcrumb-item"><a href="/Account/Login">Login</a></li>
                                            @*<li class="breadcrumb-item"><a href="/Account/RoleCreate">Create Role</a></li>
                                            <li class="breadcrumb-item"><a href="/Account/RoleAddToUser">Add Role To A User</a></li>*@


                                            @*<li class="breadcrumb-item active">Advanced Tables</li>*@
                                        </ol>
                                    </div>
                                    @*<h4 class="page-title">CYBInfrastructure</h4>*@
                                </div>
                            </div>
                        </div>
                        <!-- end page title -->
                        <div>
                            @RenderBody()
                        </div>


                    </div> <!-- container -->
                </div> <!-- content -->
                <!-- Footer Start -->
                <footer class="footer">
                    <div class="container-fluid">
                        <div class="row">
                            <div class="col-md-6">
                                CYB &copy; 2018 - CYB.com
                            </div>
                            <div class="col-md-6">
                                <div class="text-md-right footer-links d-none d-sm-block">
                                    <a href="#">About Us</a>
                                    <a href="#">Help</a>
                                    <a href="#">Contact Us</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </footer>
                <!-- end Footer -->
            </div>
            <!-- ============================================================== -->
            <!-- End Page content -->
            <!-- ============================================================== -->

        </div>
        <!-- END wrapper -->
        <!-- App js -->
        <script src="~/Scripts/jquery-3.3.1.min.js"></script>
        <script src="~/Scripts/bootstrap.min.js"></script>
        @RenderSection("scripts", false);
        <script src="~/Scripts/jquery-3.3.1.min.js"></script>
        <script src="~/Content/js/vendor.min.js"></script>
        <script src="~/Content/js/app.min.js"></script>
        <!-- Toaster js -->
        @*<script src="~/Scripts/toastr.js"></script>*@
        <script src="~/Content/js/vendor/toastr.min.js"></script>
        <script src="~/Content/js/pages/toastr.init.js"></script>
        <!-- third party js -->
        <script src="~/Content/js/vendor/jquery.dataTables.js"></script>
        <script src="~/Content/js/vendor/jquery.dataTables.js"></script>
        <script src="~/Content/js/vendor/dataTables.bootstrap4.js"></script>
        <script src="~/Content/js/vendor/dataTables.responsive.min.js"></script>
        <script src="~/Content/js/vendor/responsive.bootstrap4.min.js"></script>
        <script src="~/Content/js/vendor/dataTables.buttons.min.js"></script>
        <script src="~/Content/js/vendor/buttons.bootstrap4.min.js"></script>
        <script src="~/Content/js/vendor/buttons.html5.min.js"></script>
        <script src="~/Content/js/vendor/buttons.flash.min.js"></script>
        <script src="~/Content/js/vendor/buttons.print.min.js"></script>
        <script src="~/Scripts/notify.min.js"></script>
        <script src="~/Content/js/vendor/dataTables.keyTable.min.js"></script>
        <script src="~/Content/js/vendor/dataTables.select.min.js"></script>
        <!-- third party js ends -->
        <!-- demo app -->
        <script src="~/Content/js/pages/datatables.init.js"></script>
        <!-- end demo js-->
